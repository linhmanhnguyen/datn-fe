    <section class="product-details">
        <div class="container">
            <div class="product-buy">
                <div class="product-name-price">
                    <a class="product-item-link">
                        <%= product.data.tenSanPham %>
                    </a>

                    <div class="stars">
                        <% for (let i=0; i < Math.floor(product.data.rate); i++) { %>
                            <i class="fa-solid fa-star"></i>
                            <% } %>
                                <% if (product.data.rate % 1 !==0) { %>
                                    <i class="fa-solid fa-star-half-stroke"></i>
                                    <% } %>
                    </div>

                    <div class="price">
                        <span>
                            <%= product.data.giaBan %>
                                <%= product.data.donVi %>
                        </span>
                        <% if (product.data.sale) { %>
                            <del>
                                <%= (product.data.giaBan / (1 - parseFloat(product.data.sale))).toFixed(2) %>
                                    <%= product.data.donVi %>
                            </del>
                            <% } %>
                    </div>

                    <div class="stock-model">
                        <p>Sản phẩm: <span>
                                <%= product.data.nhanHieu %>
                            </span></p>
                        <p>Tình trạng còn: <span>Còn hàng</span></p>
                        <p>Loại: <span>Mới</span></p>
                    </div>

                    <p class="product-text">
                        <% product.data.thongTinSanPham.forEach(info=> { %>
                            <%= info %><br>
                                <% }) %>
                    </p>

                    <h6>Nhanh lên - Chỉ còn <%= product.data.stockCount || '15' %> sản phẩm trong kho.</h6>
                    <form id="myForm">
                        <button type="submit" class="add-to-cart add-to-cart2">Thêm vào giỏ hàng</button>
                        <span class="buyNow btn">Mua ngay</span>

                        <div class="quantity">
                            <span>Số lượng</span>
                            <input type="number" value="1" min="1" placeholder="Số lượng">
                        </div>
                    </form>

                    <script>
                        function getCookie(name) {
                            const value = `; ${document.cookie}`;
                            const parts = value.split(`; ${name}=`);
                            if (parts.length === 2) return parts.pop().split(';').shift();
                            return null; // Trả về null nếu không tìm thấy cookie
                        }
                        
                        // Lấy đối tượng form
                        const form = document.getElementById('myForm');
                        const quantity = document.getElementById('quantity').value;

                        // Thêm sự kiện 'submit' để ngăn chặn hành động mặc định
                        form.addEventListener('submit', function(event) {
                            event.preventDefault(); // Ngăn chặn hành động gửi form
                            console.log('Form submission disabled');
                            const userId = getCookie('userId');

                            fetch('https://cda8-2405-4803-e66f-d7b0-b45c-e2a-dd7d-1b1a.ngrok-free.app/v1/sites/cart', {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/json'
                                },
                                body: JSON.stringify({
                                    userID: userId,
                                    productID: product.data.productID,
                                    soLuong: parseInt(quantity) // Sử dụng giá trị số lượng từ input
                                })
                            })
                                .then(response => response.json())
                                .then(data => {
                                    console.log('API response:', data);
                                    alert('Đã thêm vào giỏ hàng thành công');
                                })
                                .catch(error => {
                                    console.error('Đã xảy ra lỗi:', error);
                                    alert('Đã xảy ra lỗi khi thêm vào giỏ hàng');
                                });
                        });
                    </script>

                    <ul class="product-action">
                        <li><a href="#"><i class="fa-regular fa-heart"></i></a></li>
                        <li><a href="#"><i class="fa-solid fa-sliders"></i></a></li>
                        <li><a href="#"><i class="fa-solid fa-print"></i></a></li>
                        <li><a href="#"><i class="fa-solid fa-share-nodes"></i></a></li>
                    </ul>
                </div>

                <div class="img-product">
                    <img id="big-img" src="<%= product.data.imageURL || '/publics/assets/img/home/product-9.jpg' %>"
                        alt="Product Image">

                    <div class="small-img">
