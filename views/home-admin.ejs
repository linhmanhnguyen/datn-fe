                        </div>
                        <div class="div-img">
                            <!-- Hình ảnh mặc định của sản phẩm -->
                            <img src="<%= product.imageDefault || '/publics/assets/img/home/product-default.jpg' %>" 
                                 class="img-product">
                            <!-- Hình ảnh hiển thị khi hover -->
                            <img src="<%= product.imageHover || '/publics/assets/img/home/product-hover.jpg' %>" 
                                 class="hover-img">
                        </div>
                        <div class="content">
                            <!-- Liên kết đến trang chi tiết sản phẩm -->
                            <a href="/v1/sites/buy-product/?productID=<%= product._id %>" class="product-item-link">
                                <%= product.tenSanPham %>
                            </a>
                            <div class="stars">
                                <!-- Hiển thị số lượng sao đánh giá -->
                                <% for (let i = 0; i < Math.floor(product.rate); i++) { %>
                                    <i class="fa-solid fa-star"></i>
                                <% } %>
                                <% if (product.rate % 1 !== 0) { %>
                                    <i class="fa-solid fa-star-half-stroke"></i>
                                <% } %>
                            </div>
                            <div class="price">
                                <!-- Hiển thị giá bán và đơn vị tiền tệ -->
                                <span>
                                    <%= product.giaBan.toLocaleString() %>
                                    <%= product.donVi %>
                                </span>
                                <!-- Hiển thị giá gốc đã giảm -->
                                <del>
                                    <%= (product.giaBan * 1.1).toFixed(2) %>
                                    <%= product.donVi %>
                                </del>
                            </div>
                        </div>
                    </div>
                <% }) %>
            <% } else { %>
                <!-- Thông báo khi không có sản phẩm nào -->
                <p>Không có sản phẩm nào.</p>
            <% } %>
        </div>
    
        <div class="swiper-button-next swiper-btn"></div>
        <div class="swiper-button-prev swiper-btn"></div>
    </section>
    
    

